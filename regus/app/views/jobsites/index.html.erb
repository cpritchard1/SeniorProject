<p id="notice"><%= notice %></p>

<h1>Listing Jobsites</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Street</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jobsites.each do |jobsite| %>
      <tr>
        <td><%= jobsite.name %></td>
        <td><%= jobsite.street %></td>
        <td><%= jobsite.city %></td>
        <td><%= jobsite.state %></td>
        <td><%= jobsite.zip %></td>
        <td><%= link_to 'Show', jobsite %></td>
        <td><%= link_to 'Edit', edit_jobsite_path(jobsite) %></td>
        <td><%= link_to 'Destroy', jobsite, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jobsite', new_jobsite_path %>
