              </div>
            </div>
          </div>

          <div class="row">
            <div class="large-3 columns">
              <div class="label_style">
                <h6 id="job">Agrocybe / Coprinus</h6>
              </div>
            </div>
            <div class="large-3 columns">
              <%= f.select :agrocybe, options_for_select(@category_count)%>
            </div>
            <div class="large-3 columns">
              <div class="label_style">
                <h6 id="job">Alternaria</h6>
              </div>
            </div>
            <div class="large-3 columns">
