<div id="spore_count">

  <div class="row">
    <div class="large-12 columns">
      <div class="job_type">
        <h5 id="job">Sample: <%= @tape_bulk_coc_sample.sample_num %></h5>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="large-12 columns">

