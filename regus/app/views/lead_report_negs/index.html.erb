<p id="notice"><%= notice %></p>

<h1>Listing Lead Reports Negs</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Company</th>
      <th>Service</th>
      <th>Instrument</th>
      <th>Jobsite name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lead_report_negs.each do |lead_report_neg| %>
      <tr>
        <td><%= lead_report_neg.date.strftime("%b %d %Y, %I:%M %p") %></td>
        <td><%= lead_report_neg.company %></td>
        <td><%= lead_report_neg.service %></td>
        <td><%= lead_report_neg.instrument %></td>
        <td><%= lead_report_neg.jobsite_name %></td>
        <td><%= link_to 'Show', lead_report_neg %></td>
        <td><%= link_to 'Edit', edit_lead_report_neg_path(lead_report_neg) %></td>
        <td><%= link_to 'Destroy', lead_report_neg, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
