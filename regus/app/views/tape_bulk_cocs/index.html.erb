<p id="notice"><%= notice %></p>

<h1>Listing Tape Bulk Cocs</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Turnaround</th>
      <th>Cli type</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tape_bulk_cocs.each do |tape_bulk_coc| %>
      <tr>
        <td><%= tape_bulk_coc.date %></td>
        <td><%= tape_bulk_coc.turnaround %></td>
        <td><%= tape_bulk_coc.cli_type %></td>
        <td><%= tape_bulk_coc.user %></td>
        <td><%= link_to 'Show', tape_bulk_coc %></td>
        <td><%= link_to 'Edit', edit_tape_bulk_coc_path(tape_bulk_coc) %></td>
        <td><%= link_to 'Destroy', tape_bulk_coc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

