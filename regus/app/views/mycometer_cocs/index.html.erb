<p id="notice"><%= notice %></p>

<h1>Listing Mycometer Cocs</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Turnaround</th>
      <th>Mold air</th>
      <th>Building type</th>
      <th>Sampling method</th>
      <th>Ambient temp</th>
      <th>Reaction time</th>
      <th>Liters minute</th>
      <th>Duration</th>
      <th>Job type</th>
      <th>Cli type</th>
      <th>Job</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mycometer_cocs.each do |mycometer_coc| %>
      <tr>
        <td><%= mycometer_coc.date %></td>
        <td><%= mycometer_coc.turnaround %></td>
        <td><%= mycometer_coc.mold_air %></td>
        <td><%= mycometer_coc.building_type %></td>
        <td><%= mycometer_coc.sampling_method %></td>
        <td><%= mycometer_coc.ambient_temp %></td>
        <td><%= mycometer_coc.reaction_time %></td>
        <td><%= mycometer_coc.liters_minute %></td>
        <td><%= mycometer_coc.duration %></td>
        <td><%= mycometer_coc.job_type %></td>
        <td><%= mycometer_coc.cli_type %></td>
        <td><%= mycometer_coc.job %></td>
        <td><%= mycometer_coc.user %></td>
        <td><%= link_to 'Show', mycometer_coc %></td>
        <td><%= link_to 'Edit', edit_mycometer_coc_path(mycometer_coc) %></td>
        <td><%= link_to 'Destroy', mycometer_coc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mycometer coc', new_mycometer_coc_path %>
